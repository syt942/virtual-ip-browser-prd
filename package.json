{
  "name": "virtual-ip-browser",
  "version": "1.2.1",
  "description": "Enhanced Privacy-Focused Browser with Proxy Management and Fingerprint Spoofing",
  "main": "out/main/index.js",
  "type": "module",
  "author": {
    "name": "Development Team",
    "email": "dev@virtualipbrowser.com"
  },
  "homepage": "https://github.com/virtualipbrowser/virtual-ip-browser",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:e2e": "playwright test",
    "package": "electron-builder",
    "package:win": "electron-builder --win",
    "package:mac": "electron-builder --mac",
    "package:linux": "electron-builder --linux"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "electron-store": "^8.2.0",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.453.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^2.15.2",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "electron": "^35.0.0",
    "electron-builder": "^26.0.0",
    "electron-vite": "^3.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-security": "^3.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.3",
    "vite": "^6.2.0",
    "vitest": "^1.6.0"
  },
  "build": {
    "appId": "com.virtualipbrowser.app",
    "productName": "Virtual IP Browser",
    "directories": {
      "output": "release",
      "buildResources": "resources"
    },
    "files": [
      "out/**/*",
      "resources/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities",
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "resources/icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "resources/icons",
      "maintainer": "dev@virtualipbrowser.com",
      "vendor": "Virtual IP Browser Development Team",
      "synopsis": "Privacy-Focused Browser with Proxy Management",
      "description": "Enhanced Privacy-Focused Browser with Proxy Management and Fingerprint Spoofing"
    }
  }
}
